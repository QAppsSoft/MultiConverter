<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:MultiConverter"
             xmlns:materialIcons="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             x:Class="MultiConverter.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
    <Application.Styles>
        <FluentTheme Mode="Dark" DensityStyle="Normal"/>
        <materialIcons:MaterialIconStyles />
        <StyleInclude Source="avares://MultiConverter/Styles/MainMenuIcons.axaml" />
        <StyleInclude Source="avares://MultiConverter/Styles/Image.axaml" />
        <StyleInclude Source="avares://MultiConverter/Styles/Icons.axaml" />
    </Application.Styles>
</Application>
