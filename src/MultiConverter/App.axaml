<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:MultiConverter"
             xmlns:styling="clr-namespace:FluentAvalonia.Styling;assembly=FluentAvalonia"
             x:Class="MultiConverter.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <styling:FluentAvaloniaTheme />
        <StyleInclude Source="/Styles/OptionsDisplayItemStyles.axaml" />
        <StyleInclude Source="/Styles/OptionsExpanderSeparatorStyles.axaml" />
        <StyleInclude Source="/Styles/OptionsDisplayExpanderItemStyles.axaml" />
    </Application.Styles>
</Application>
